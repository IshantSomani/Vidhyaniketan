<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <FrameLayout
        android:id="@+id/photography_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingTop="20dp"
        android:background="@android:color/transparent">

        <pl.droidsonroids.gif.GifImageView
            android:id="@+id/gifImageView19"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/photography"
            android:scaleType="centerCrop"
            android:background="@android:color/transparent" />
    </FrameLayout>

    <TextView
        android:id="@+id/photography_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/photography"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:textStyle="bold" />

    <FrameLayout
        android:id="@+id/cookery1_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingTop="20dp"
        android:background="@android:color/transparent">

        <pl.droidsonroids.gif.GifImageView
            android:id="@+id/gifImageView21"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/cookery"
            android:scaleType="centerCrop"
            android:background="@android:color/transparent" />
    </FrameLayout>

    <TextView
        android:id="@+id/cookery1_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/cookery"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:textStyle="bold" />

    <FrameLayout
        android:id="@+id/cyclerepair_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingTop="20dp"
        android:background="@android:color/transparent">

        <pl.droidsonroids.gif.GifImageView
            android:id="@+id/gifImageView22"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/cyclerepair"
            android:scaleType="centerCrop"
            android:background="@android:color/transparent" />
    </FrameLayout>

    <TextView
        android:id="@+id/cyclerepair_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/cycle_repair"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:textStyle="bold" />

    <FrameLayout
        android:id="@+id/twowheelerre_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingTop="20dp"
        android:background="@android:color/transparent">

        <pl.droidsonroids.gif.GifImageView
            android:id="@+id/gifImageView23"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/twowheelerre"
            android:scaleType="centerCrop"
            android:background="@android:color/transparent" />
    </FrameLayout>

    <TextView
        android:id="@+id/twowheelerre_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/two_wheeler_repair"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:textStyle="bold" />

    <FrameLayout
        android:id="@+id/crochet_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:paddingTop="20dp"
        android:background="@android:color/transparent">

        <pl.droidsonroids.gif.GifImageView
            android:id="@+id/gifImageView24"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/crochet"
            android:scaleType="centerCrop"
            android:background="@android:color/transparent" />
    </FrameLayout>

    <TextView
        android:id="@+id/crochet_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/crochet"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        android:textStyle="bold" />
</LinearLayout>